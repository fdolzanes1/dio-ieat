<h1>Products</h1>

<table>
	<thead>
		<tr>
			<th> Name </th>
			<th> Valor </th>
			<th> Restaurante </th>
		</tr>
	</thead>
	<tbody>
		<% @products.each do |p| %>
		<tr>
			<th> <%= p.name %></th>
			<th> <%= p.value %></th>
			<th> <%= p.restaurant.name %></th>
		</tr>
		<% end %>
	</tbody>

</table>